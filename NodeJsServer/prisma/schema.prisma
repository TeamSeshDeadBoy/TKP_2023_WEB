generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id             String @id @map("_id")
  email          String @unique
  name           String
  password       String
  quizzes        Quiz[]
  currentQuizInd Int?
}

type Quiz {
  title     String
  Questions Question[]
}

type Question {
  text      String
  isCorrect Boolean
}
